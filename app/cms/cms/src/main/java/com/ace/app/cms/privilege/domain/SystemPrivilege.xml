<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.ace.app.cms.privilege.domain.SystemPrivilege">
    <!-- Result Map-->
    <resultMap id="BaseResultMap" type="com.ace.app.cms.privilege.domain.SystemPrivilege">
        <result column="sp_id" property="spId"/>
        <result column="module" property="module"/>
        <result column="privilege" property="privilege"/>
        <result column="name" property="name"/>
        <result column="corp_code" property="corpCode"/>
        <result column="create_by" property="createBy"/>
        <result column="create_time" property="createTime"/>
        <result column="last_modify_by" property="lastModifyBy"/>
        <result column="last_modify_time" property="lastModifyTime"/>
    </resultMap>

    <!-- t_cms_role table all fields -->
    <sql id="Base_Column_List">
        sp_id,module,privilege,name,corp_code,create_by,create_time,last_modify_by,last_modify_time
    </sql>

    <!-- 查询条件 -->
    <sql id="Example_Where_Clause">
        where 1=1
        <trim suffixOverrides=",">
            <if test="spId != null and spId != ''">
            and sp_id = #{ spId }
        </if>
            <if test="module != null and module != ''">
            and module = #{ module }
        </if>
            <if test="privilege != null and privilege != ''">
            and privilege = #{ privilege }
        </if>
            <if test="name != null and name != ''">
            and name = #{ name }
        </if>
            <if test="corpCode != null and corpCode != ''">
            and corp_code = #{ corpCode }
        </if>
            <if test="createBy != null and createBy != ''">
            and create_by = #{ createBy }
        </if>
            <if test="createTime != null ">
            and create_time = #{ createTime }
        </if>
            <if test="lastModifyBy != null and lastModifyBy != ''">
            and last_modify_by = #{ lastModifyBy }
        </if>
            <if test="lastModifyTime != null ">
            and last_modify_time = #{ lastModifyTime }
        </if>
        </trim>
    </sql>


    <!-- 插入记录 -->
    <insert id="insert" parameterType="com.ace.app.cms.privilege.domain.SystemPrivilege">
        insert into t_cms_systemprivilege(sp_id,module,privilege,name,corp_code,create_by,create_time,last_modify_by,last_modify_time)
 values(#{spId},#{module},#{privilege},#{name},#{corpCode},#{createBy},now(),#{lastModifyBy},#{lastModifyTime})
    </insert>

    <!-- 根据id，修改记录-->
    <update id="update" parameterType="com.ace.app.cms.privilege.domain.SystemPrivilege">
        update t_cms_systemprivilege set module=#{module},privilege=#{privilege},name=#{name},corp_code=#{corpCode},create_by=#{createBy},create_time=now(),last_modify_by=#{lastModifyBy},last_modify_time=#{lastModifyTime} where sp_id=#{spId}
    </update>

    <!-- 修改记录，只修改只不为空的字段 -->
    <update id="updateBySelective" parameterType="com.ace.app.cms.privilege.domain.SystemPrivilege">
    update t_cms_systemprivilege set 
	<trim  suffixOverrides="," >
	<if test="module != null  and module != '' ">
		module=#{module},
	</if>
	<if test="privilege != null  and privilege != '' ">
		privilege=#{privilege},
	</if>
	<if test="name != null  and name != '' ">
		name=#{name},
	</if>
	<if test="corpCode != null  and corpCode != '' ">
		corp_code=#{corpCode},
	</if>
	<if test="createBy != null  and createBy != '' ">
		create_by=#{createBy},
	</if>
	<if test="createTime != null  ">
		create_time=#{createTime},
	</if>
	<if test="lastModifyBy != null  and lastModifyBy != '' ">
		last_modify_by=#{lastModifyBy},
	</if>
	<if test="lastModifyTime != null  ">
		last_modify_time=#{lastModifyTime},
	</if>
	</trim> where sp_id=#{spId}
    </update>

    <!-- 删除记录 -->
    <delete id="delete" parameterType="String">
    delete 	 from t_cms_systemprivilege where sp_id = #{spId}
    </delete>

    <!-- 根据id查询 角色 -->
    <select id="getObject" resultMap="BaseResultMap" parameterType="String">
    select <include refid="Base_Column_List" /> 
	 from t_cms_systemprivilege where sp_id = #{spId}
    </select>


    <!-- 角色列表总数-->
    <select id="getCount" resultType="java.lang.Integer">
        select count(1) from t_cms_systemprivilege
    </select>

    <!-- 角色条件列表总数-->
    <select id="getCountBySelective" resultType="java.lang.Integer" parameterType="com.ace.app.cms.privilege.domain.SystemPrivilege">
        select count(1) from t_cms_systemprivilege
        <include refid="Example_Where_Clause"/>
    </select>

    <!-- 查询角色列表 -->
    <select id="findList" resultMap="BaseResultMap" parameterType="com.ace.app.cms.privilege.domain.SystemPrivilege">
        select
        <include refid="Base_Column_List"/>
        from t_cms_systemprivilege
        <include refid="Example_Where_Clause"/>
    </select>

</mapper>
